{
  "manifest_version": 3,
  "name": "ClipNotes",
  "version": "1.0.0",
  "description": "Your note taking concierge for your browser window",
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "index.html",
    "default_icon": "vite.svg"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["assets/index-DrV0-QIx.js"]
    }
  ]
}